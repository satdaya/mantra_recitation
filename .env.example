# React App Environment Variables
# Copy this file to .env and update with your values

# Backend API URL
REACT_APP_API_URL=http://localhost:8000

# Google Sheets Integration
# Get these from: https://console.cloud.google.com/apis/credentials
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_GOOGLE_API_KEY=your_google_api_key_here
# Get Sheet ID from the URL: https://docs.google.com/spreadsheets/d/SHEET_ID/edit
REACT_APP_GOOGLE_SHEET_ID=your_google_sheet_id_here

# Development settings
GENERATE_SOURCEMAP=false

# Optional: Airtable Integration (if using)
# REACT_APP_AIRTABLE_BASE_ID=your_base_id
# REACT_APP_AIRTABLE_API_KEY=your_api_key